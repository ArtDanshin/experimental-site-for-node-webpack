{
  "name": "@artdanshin/ui",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/es/components/*/index.js",
      "require": "./dist/cjs/components/*/index.cjs"
    },
    "./styles": {
      "import": "./dist/es/ui.css",
      "require": "./dist/cjs/ui.css"
    }
  },
  "sideEffects": [
    "**/*.css",
    "dist/es/components/*/index.js",
    "dist/cjs/components/*/index.cjs",
    "dist/es/tokens/index.js",
    "dist/cjs/tokens/index.cjs"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "lint": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^9.1.9",
    "@storybook/addon-docs": "^9.1.9",
    "@storybook/vue3-vite": "^9.1.9",
    "@types/node": "^24.6.2",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.36.0",
    "eslint-config-artdanshin": "*",
    "playwright": "^1.55.1",
    "storybook": "^9.1.9",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.7"
  }
}
